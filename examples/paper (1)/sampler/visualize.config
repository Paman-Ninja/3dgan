# conditional, sampling GAN (for paper)

# model
model		   paper_cgan
noise_layer    x

# data
dataset		   nyuv2
random_crop    65 65
n_threads      6
# skip_invalid
include_originals 427 561



# training
epochs		   50
batch_size 	   512
n_gpus 		   2
optimizer	   adam
g_lr		   1e-5
g_beta1		   0.5
d_lr		   1e-5
d_beta1		   0.5
